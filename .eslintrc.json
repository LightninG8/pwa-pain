{
  "env": {
      "browser": true,
      "es6": true
  },
  "extends": [
      "plugin:react/recommended",
      "google"
  ],
  "globals": {
      "React": true,
      "Atomics": "readonly",
      "SharedArrayBuffer": "readonly"
  },
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
      "ecmaFeatures": {
          "jsx": true
      },
      "ecmaVersion": 2018,
      "sourceType": "module"
  },
  "plugins": [
      "react",
      "@typescript-eslint"
  ],
  "rules": {
      "quote-props": "off",
      "linebreak-style": 0,
      "operator-linebreak": "off",
      "react/prop-types": "warn",
      "object-curly-spacing": "off",
      "comma-dangle": "off",
      "indent": [
          "warn",
          2,
          {
              "SwitchCase": 1,
              "offsetTernaryExpressions": true
          }
      ],
      "max-len": [
          1,
          {
              "code": 125
          }
      ],
      "no-unused-vars": "warn",
      "curly": "warn",
      "array-bracket-spacing": "warn",
      "prefer-const": "warn",
      "camelcase": "warn",
    //   "no-console": "warn",
      "valid-jsdoc": "warn",
      "quotes": [
          "error",
          "single"
      ],
      "no-var": "error",
      "function-paren-newline": "off",
      "react/jsx-no-target-blank": 1,
      "require-jsdoc": "off"
  }
}